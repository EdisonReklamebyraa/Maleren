  color: $darkblue;
  font-size: 12px;
  line-height: 16px;
  padding: 7px; }

.leadin {
  margin-bottom: 19px;
  font-size: 16px;
  line-height: 20px; }



.assets {
  font-size: 12px;
  line-height: 17px;
  margin-bottom: 12px; }

.searchresult {
  padding: 10px 0px;
  margin-bottom: 10px;
  background: #f7f8f8;
  border-bottom: 1px solid $light-gray;
  line-height: 16px; }

.video-container {
  border: 1px solid $light-gray; }

.searchinfo {
  width: 50%;
  text-align: center;
  float: left; }

.searchresult {
  a {
    text-decoration: none;
    &:hover {
      text-decoration: underline; }
    &.next {
      margin-left: 10px;
      margin-right: 2px; }
    &.prev {
      margin-right: 10px;
      margin-left: 2px; } }
  .next-wrapper {
    float: right;
    font-weight: bold;
    width: 25%; }
  .prev-wrapper {
    float: left;
    font-weight: bold; 
    width: 25%;} }

.box .nobg {
  background: none; }



.sdz {
  text-align: center;
  background: #373535;
  height: 100px;
  vertical-align: middle;
  display: table-cell; }

.type-2, .type-3 {
  border-bottom: 0;
  padding: 0 2px;
  margin-bottom: 0;
  /*	padding:0;
   *	border:none;
   *	color:#2e5775; */ }

.type-2 h2, .type-3 h2 {
  margin: 0 0 0 -2px; }

.col-3 {
  .type-2 h2, .type-3 h2 {
    border: none;
    /*	background:url(../images/box-type2-header-bg.jpg) no-repeat; */
    color: #2b2b2b;
    /*	font-size:18px;
     *	font-family:"Times New Roman", Times, serif; */ } }

.type-2 .box-content h3, .type-3 .box-content h3 {
  font-size: 16px;
  font-family: Arial, Helvetica, sans-serif;
  color: $darkblue;
  line-height: 22px; }

.type-2 .box-content h3.georgia, .type-3 .box-content h3.georgia {
  font-family: Georgia, "Times New Roman", Times, serif; }

.type-2 .box-content p, .type-3 .box-content p {
  font-size: 12px;
  color: $darkblue;
  margin-bottom: 12px; }

.type-2 .box-content {
  .article-intro h3 a {
    /*	color:#131313; */ }
  a.arrow {
    margin-bottom: 6px; } }

.type-3 .box-content a.arrow {
  margin-bottom: 6px; }

.type-2 .has-image .image-wrapper, .type-3 .has-image .image-wrapper {
  float: left;
  width: 110px;
  padding-top: 3px; }

.type-2 .has-image .content-wrapper, .type-3 .has-image .content-wrapper {
  float: left;
  width: 160px; }

.type-2 .box-content {
  background: url(../images/type-2-bg.jpg) repeat-x bottom;
  padding-bottom: 13px; }

.type-3 .box-content {
  background: url(../images/type-3-bg.jpg) repeat-x bottom;
  border-left: 1px solid #e1e1e0;
  border-right: 1px solid #e1e1e0;
  padding-bottom: 6px; }

.type-2 .box-content .padder, .type-3 .box-content .padder {
  padding: 5px 5px 0 5px; }

.box-type-2-footer {
  margin: 0;
  width: auto;
  height: 17px;
  background: url(../images/type-2-footer.gif) no-repeat;
  margin-bottom: 10px; }



.box .box-content .one-line {
  margin-bottom: 0.5em;
  a {} }

.no-bg {
  background: none !important; }

.googlemap, .map-container {
  margin-bottom: 10px; }
